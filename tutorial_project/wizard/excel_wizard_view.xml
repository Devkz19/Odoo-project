<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record model="ir.ui.view" id="rest_staff_wizard_excel_form_view">
        <field name="name">rest.staff.wizard.excel.form.view.id</field>
        <field name="model">custom.excel.wizard</field>
        <field name="arch" type="xml">
            <form string="Staff Form">
                <group>
                    <field name="gender"></field>
                </group>
                <footer>
                    <button name="print_excel" string="Print Excel" type="object"
                        class="oe_highlight" />
                    <button name="print_excel_report_by_menu" string="Print Excel(M)" type="object"
                        class="oe_highlight" />    
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <record id="rest_staff_wizard_action_excel_report" model="ir.actions.act_window">
        <field name="name">Staff Excel Report</field>
        <field name="res_model">custom.excel.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
        <field name="view_id" ref="tutorial_project.rest_staff_wizard_excel_form_view" />
    </record>

</odoo>